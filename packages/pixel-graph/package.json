    "pixel",
    "pixelated",
    "area-chart",
    "data-visualization",
    "svg",
    "typescript"
  ],
  "author": "Pushpal Ghoshal <pushpal_g@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Atomized-titan/pixel-graph.git",
    "directory": "packages/pixel-graph"
  },
  "bugs": {
    "url": "https://github.com/Atomized-titan/pixel-graph/issues"
  },
  "homepage": "https://github.com/Atomized-titan/pixel-graph#readme",
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --external react,react-dom",
    "dev": "tsup src/index.tsx --format cjs,esm --dts --watch --external react,react-dom",
    "lint": "eslint src/"
  },
  "peerDependencies": {
    "react": ">=18",
