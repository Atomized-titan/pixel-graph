{
  "name": "pixel-graph-react",
  "version": "0.1.0",
  "description": "A beautiful, customizable pixelated area chart component for React",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
